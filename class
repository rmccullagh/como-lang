class Container {
	func Container(obj) {
		self.properties = obj;
		self.getProperties().name = "Ryan McCullagh";
  }
	func setProperties(obj) {
		self.properties = obj;
	}
	func getProperties() {
		return self.properties;
	}
}

container = new Container(new Object());

props = container.getProperties();

print(props.name);

